{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/en_kjv.json",
      "dest": "/index.js"
    },
    {
      "src": "/crossRefs.json",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|ico|json))",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}